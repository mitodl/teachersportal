{
  "parser": "babel-eslint",
  "extends": "defaults/configurations/google",
  "rules": {
    "quotes": [0],
    "object-curly-spacing": [0],
    "new-cap": [0]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "babel",
    "react"
  ]
}
